{
  "name": "flatbush",
  "version": "3.3.0",
  "description": "Fast static spatial index for rectangles",
  "main": "build/flatbush.js",
  "module": "build/flatbush",
  "unpkg": "build/flatbush.min.js",
  "jsdelivr": "build/flatbush.min.js",
  "scripts": {
    "pretest": "yarn lint",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test.ts'",
    "lint": "eslint ./src --ext .jsx,.js,.tsx,.ts --max-warnings 0",
    "bench": "ts-node bench.ts",
    "build": "rollup -c",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "src/index",
    "build/flatbush.js",
    "build/flatbush.min.js"
  ],
  "@std/esm": "js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mourner/flatbush.git"
  },
  "keywords": [
    "geometry",
    "spatial",
    "tree",
    "index",
    "rectangle",
    "search"
  ],
  "author": "Vladimir Agafonkin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mourner/flatbush/issues"
  },
  "homepage": "https://github.com/mourner/flatbush#readme",
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@tsconfig/node12": "^1.0.7",
    "@types/assert": "^1.5.2",
    "@types/expect": "^24.3.0",
    "@types/jest": "^26.0.15",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.0",
    "@types/tape": "^4.13.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "@wessberg/rollup-plugin-ts": "^1.3.5",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "esm": "^3.2.25",
    "mocha": "^8.2.0",
    "rbush": "^3.0.1",
    "rbush-knn": "^3.0.1",
    "rollup": "^2.10.7",
    "rollup-plugin-terser": "^7.0.2",
    "tape": "^4.13.2",
    "ts-mocha": "^7.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "flatqueue": "thesulaj/flatqueue"
  }
}
